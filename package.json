    "@elysiajs/server-timing": "^1.4.0",
    "@react-email/components": "^1.0.1",
    "bullmq": "^5.65.1",
    "elysia": "latest",
    "elysia-healthcheck": "^1.0.0",
    "elysia-xss": "^1.0.3",
    "logixlysia": "^5.3.0",
    "nodemailer": "^7.0.11",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@react-email/preview-server": "5.0.5",
